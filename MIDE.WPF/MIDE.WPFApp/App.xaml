<Application x:Class="MIDE.WPF.WPFApplication"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:MIDE.WPF">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/shared;component/Styles/Shared.xaml"/>
                <ResourceDictionary Source="/shared;component/Styles/Controls.xaml" />
                <ResourceDictionary Source="/shared;component/Styles/Colors.xaml" />
                <ResourceDictionary Source="/shared;component/Styles/Brushes.xaml" />
                <ResourceDictionary Source="/shared;component/Styles/Fonts.xaml" />
                <ResourceDictionary Source="Styles/Windows.xaml" />
                <ResourceDictionary Source="/shared;component/Styles/DefaultControls.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>