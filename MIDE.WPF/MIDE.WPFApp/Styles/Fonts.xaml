                    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <!-- <FontFamily x:Key="YourFontFamily">pack://application;,,,/Fonts/#Font Name</FontFamily> -->
    <!-- In the UI-component's attributes write: FontFamily="{StaticResourse YourFontFamily}" -->
    <!-- To make a default:
    <Style TargetType="{x:Type Control}" x:Key="BaseStyle">
        <Setter Property="FontFamily" Value="{StaticResource YourFontFamily}"/>
    </Style>
    <Style TargetType="{x:Type Button}" BasedOn="{StaticResource BaseStyle}"/> -->

    <FontFamily x:Key="FontAwesome">pack://application;,,,/Fonts/#FontAwesome</FontFamily>

