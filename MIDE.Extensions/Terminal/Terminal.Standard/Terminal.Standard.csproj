<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>Terminal</RootNamespace>
    <AssemblyName>terminal</AssemblyName>
    <Authors>Andrew Satura</Authors>
    <Company>Vardir Products</Company>
    <Product>MIDE Terminal</Product>
    <Description></Description>
    <Copyright>Copyright 2019 Â© Andrew Satura</Copyright>
    <RunPostBuildEvent>OnOutputUpdated</RunPostBuildEvent>
    <PackageId>MIDE.Terminal</PackageId>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageLicenseExpression></PackageLicenseExpression>
    <PackageIconUrl></PackageIconUrl>
    <PackageTags></PackageTags>
    <PackageReleaseNotes></PackageReleaseNotes>
    <RepositoryType></RepositoryType>
    <RepositoryUrl></RepositoryUrl>
    <PackageProjectUrl></PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="icon.png" />
    <None Remove="Terminal.nuspec" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="icon.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="Terminal.nuspec">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <Reference Include="api">
      <HintPath>..\..\..\Publish\MIDE.Kernel\api.dll</HintPath>
    </Reference>
    <Reference Include="FSharp.Core">
      <HintPath>..\..\..\..\..\..\..\Server\Publish\MyCMD\FSharp.Core.dll</HintPath>
    </Reference>
    <Reference Include="mycmd.kernel">
      <HintPath>..\..\..\..\..\..\..\Server\Publish\MyCMD\mycmd.kernel.dll</HintPath>
    </Reference>
    <Reference Include="Parser">
      <HintPath>..\..\..\..\..\..\..\Server\Publish\MyCMD\Parser.dll</HintPath>
    </Reference>
  </ItemGroup>

  <Target Name="PostBuild" AfterTargets="PostBuildEvent">
    <Exec Command="set path=$(SolutionDir)NuPkg\&#xD;&#xA;if not exist %25path%25 mkdir %25path%25&#xD;&#xA;copy &quot;$(TargetDir)$(TargetFileName)&quot; %25path%25&#xD;&#xA;copy &quot;$(TargetDir)FSharp.Core.dll&quot; %25path%25&#xD;&#xA;copy &quot;$(TargetDir)Parser.dll&quot; %25path%25&#xD;&#xA;copy &quot;$(TargetDir)mycmd.kernel.dll&quot; %25path%25&#xD;&#xA;copy &quot;$(TargetDir)Terminal.nuspec&quot; %25path%25&#xD;&#xA;copy &quot;$(TargetDir)icon.png&quot; %25path%25" />
  </Target>

</Project>
